{
  "name": "ghx-router",
  "version": "1.0.0",
  "private": true,
  "description": "CLI-first GitHub execution router for agents.",
  "packageManager": "pnpm@10.17.0",
  "scripts": {
    "test": "pnpm -r run test",
    "typecheck": "pnpm -r run typecheck",
    "verify": "pnpm run test && pnpm run typecheck",
    "benchmark:check": "pnpm --filter @ghx-router/benchmark run check:scenarios",
    "benchmark:run": "pnpm --filter @ghx-router/benchmark run run",
    "benchmark:report": "pnpm --filter @ghx-router/benchmark run report",
    "benchmark:gate": "pnpm --filter @ghx-router/benchmark run report:gate",
    "benchmark:typecheck": "pnpm --filter @ghx-router/benchmark run typecheck",
    "ghx:typecheck": "pnpm --filter @ghx-router/core run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryeko/ghx-router.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/aryeko/ghx-router/issues"
  },
  "homepage": "https://github.com/aryeko/ghx-router#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3"
  }
}
